{
  "name": "vicui",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/vicui.cjs.js",
  "module": "dist/vicui.es.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0"
  },
  "scripts": {
    "build": "yarn run lint && yarn run clean:dist && cross-env NODE_ENV=production rollup -c",
    "clean:dist": "rimraf dist",
    "watch": "rollup -cw",
    "start": "rollup -cw",
    "lint": "tslint '{src,tests}/**/*.{ts,tsx}' --fix",
    "test": "",
    "release": "yarn run build && standard-version",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/jest": "24.0.12",
    "@types/node": "11.13.8",
    "@types/react": "16.8.15",
    "@types/react-dom": "16.8.4",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-info": "^4.1.1",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.14",
    "@types/styled-system": "^4.1.2",
    "babel-loader": "^8.0.5",
    "react-docgen-typescript-loader": "^3.1.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-typescript2": "^0.21.0",
    "standard-version": "^5.0.2",
    "storybook-addon-styled-component-theme": "^1.2.1",
    "tslint": "^5.16.0",
    "tslint-react": "^4.0.0",
    "typescript": "3.3.4000"
  }
}
